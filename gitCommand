git status
git log
git log --oneline
git log --oneline | wc -git
git log --oneline --graph
git log --graph --oneline --all --decorate
git shortlog -sne
git add -A/-u
git add -am "Sharing is fun"

git commit -m "comment"
git diff HEAD~1..HEAD
git checkout file.txt
git reset --hard
git reset --soft HEAD~1

git clean -n // will list files to clean repo
git clone

git show HEAD

git remote
git branch feature1 // creates feature1 branch
git branch -r // will display remote branches
git checkout feature1 // switches to branch feature1

git tag
git push --tags

git fetch // fetch changes from repo

git push
git pull

git config --global alias.lga "log --graph --oneline --all --decorate" //creates alias
